		status = "disabled";
	};
};

&soc {
	/* sound passthrough */
	i2c2_clk: i2c2_clk {
		compatible = "fixed-clock";
		/*
		The value "133333328" was taken from /sys/kernel/debug/clk/clk_summary
		for the corresponding clock in DomD.
		*/
		clock-frequency = <133333328>;
		#clock-cells = <0>;
	};

	fixed_clk_66560000: fixed_clk_66560000 {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <66560000>;
	};
	fixed_serial_clk: fixed_serial_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <266666656>;
	};
	fixed_clk_scif1: fixed_clk_scif1 {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <66666664>;
	};


